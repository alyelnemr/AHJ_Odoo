<odoo>
    <data>
        <record id="aly_action_oasis_room" model="ir.actions.act_window">
            <field name="name">Rooms</field>
            <field name="res_model">oasis.rooms</field>
            <field name="binding_view_types">form</field>
            <field name="view_mode">tree,form</field>
        </record>
    </data>
    <!-- explicit list view definition -->
    <record id="aly_oasis_rooms_list" model="ir.ui.view">
      <field name="name">Rooms</field>
      <field name="model">oasis.rooms</field>
      <field name="arch" type="xml">
        <tree>
          <field name="room_no"/>
          <field name="room_name"/>
        </tree>
      </field>
    </record>

</odoo>